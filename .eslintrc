{
    "parser": "@babel/eslint-parser",
    "extends" : ["airbnb", "prettier"],
    "rules": {
    "react/jsx-filename-extension": [1, {
    "extensions": [".js"]
    }],
    "indent": ["error", 4],
    "react/jsx-indent": ["error", 4],
    "react/jsx-indent-props": ["error", 4],
    "max-len": ["error", { "code": 100}],
    "quotes": ["error", "single", { "avoidEscape": true}]
    },
    "globals": {
    "window": true,
    "document": true
    }
    }